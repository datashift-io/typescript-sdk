{
  "name": "@datashift/sdk",
  "version": "0.3.0",
  "description": "Datashift SDK for human-in-the-loop AI agent workflows",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest --passWithNoTests",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "datashift",
    "ai",
    "agents",
    "human-in-the-loop",
    "hitl"
  ],
  "author": "Datashift",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/datashift-io/typescript-sdk.git"
  },
  "homepage": "https://datashift.io",
  "bugs": {
    "url": "https://github.com/datashift-io/typescript-sdk/issues"
  },
  "dependencies": {
    "axios": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "eslint": "^10.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.56.0"
  },
  "overrides": {
    "minimatch": ">=10.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
